    "def deco(func):\n",
    "    def wrapper(*arg):\n",
    "        if type(*arg) == int:        \n",
    "            ret = func(*arg)\n",
    "            return ret\n",
    "        else:\n",
    "            return [func(a) for a in arg[0]]\n",
    "\n",
    "    return wrapper\n",
    "\n",
    "\n",
    "@deco\n",
    "def nijo(n):\n",
    "    return n**2\n",
    "\n",
    "x = 2\n",
    "answer = nijo(x)\n",
    "print(answer)"
   ]
